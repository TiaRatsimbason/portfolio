{
    "version": 2,
    "builds": [
      {
        "src": "index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.py"
      }
    ],
    "env": {
      "FLASK_ENV": "development",
      "FLASK_DEBUG": "1"
    }
  }